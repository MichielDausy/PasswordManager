<Window x:Class="PasswordManager.AddGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManager"
        mc:Ignorable="d"
        Title="AddGroup" Height="450" Width="300">
    <Grid>
        <Label x:Name="accountLabel" Content="Group Name:" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,232,0,0"/>
        <TextBox x:Name="GroupText" HorizontalAlignment="Center" Margin="0,258,0,0" TextWrapping="Wrap" Width="200" RenderTransformOrigin="0.33,0.726" VerticalAlignment="Top"/>
        <Button x:Name="SubmitGroup" Content="Submit" HorizontalAlignment="Left" Margin="50,335,0,0" VerticalAlignment="Top" Width="200" Click="SubmitGroup_Click"/>
        <Label x:Name="Create_new_group" Content="Create new group" HorizontalAlignment="Center" Margin="0,65,0,0" VerticalAlignment="Top" Width="200" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
